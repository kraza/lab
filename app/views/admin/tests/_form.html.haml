  -if @admin_test.errors.any?
    #error_explanation
      %h2= "#{pluralize(@admin_test.errors.count, "error")} prohibited this admin_test from being saved:"
      %ul
        - @admin_test.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :category_id
    = f.text_field :category_id
  .field
    = f.label :code
    = f.text_field :code
  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :fees
    = f.text_field :fees
  .field
    = f.label :commission_type
    = f.text_field :commission_type
  .field
    = f.label :commissin_value
    = f.text_field :commissin_value
  .actions
    = f.submit 'Save'
