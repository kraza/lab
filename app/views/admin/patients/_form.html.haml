= form_for @admin_patient do |f|
  -if @admin_patient.errors.any?
    #error_explanation
      %h2= "#{pluralize(@admin_patient.errors.count, "error")} prohibited this admin_patient from being saved:"
      %ul
        - @admin_patient.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :refrence_no
    = f.text_field :refrence_no
  .field
    = f.label :initial_name
    = f.text_field :initial_name
  .field
    = f.label :first_name
    = f.text_field :first_name
  .field
    = f.label :last_name
    = f.text_field :last_name
  .field
    = f.label :admin_doctor_id
    = f.text_field :admin_doctor_id
  .field
    = f.label :test_date
    = f.date_select :test_date
  .field
    = f.label :test_delivery_date
    = f.date_select :test_delivery_date
  .field
    = f.label :total_amount
    = f.text_field :total_amount
  .field
    = f.label :advance_payment
    = f.text_field :advance_payment
  .field
    = f.label :is_doctor_received_payment
    = f.check_box :is_doctor_received_payment
  .actions
    = f.submit 'Save'
